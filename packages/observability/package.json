{
  "name": "@ryuu/observability",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    "./status": {
      "types": "./dist/src/status/index.d.ts",
      "default": "./src/status/index.ts"
    },
    "./error": {
      "types": "./dist/src/error.d.ts",
      "default": "./src/error.ts"
    },
    "./log": {
      "types": "./dist/src/log.d.ts",
      "default": "./src/log.ts"
    },
    "./nextjs": {
      "types": "./dist/src/next.config.d.ts",
      "default": "./src/next.config.ts"
    },
    "./env": {
      "types": "./dist/env.d.ts",
      "default": "./env.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsc",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "prettier": "@ryuu/prettier-config",
  "dependencies": {
    "@ryuu/env": "workspace:*",
    "@sentry/nextjs": "^9.16.1",
    "@t3-oss/env-nextjs": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@ryuu/auth": "workspace:*",
    "@ryuu/eslint-config": "workspace:*",
    "@ryuu/prettier-config": "workspace:*",
    "@ryuu/tsconfig": "workspace:*",
    "@vercel/functions": "^2.0.2",
    "eslint": "catalog:",
    "next": "catalog:next",
    "prettier": "catalog:",
    "typescript": "catalog:"
  }
}
