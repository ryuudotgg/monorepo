{
  "$schema": "https://turborepo.org/schema.json",
  "ui": "tui",
  "tasks": {
    "topo": {
      "dependsOn": ["^topo"]
    },
    "analyze": {
      "dependsOn": ["^analyze"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        ".cache/tsbuildinfo.json",
        "dist/**"
      ]
    },
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    },
    "dev": {
      "dependsOn": ["^dev"],
      "cache": false,
      "persistent": false
    },
    "format": {
      "outputs": [".cache/.prettiercache"],
      "outputLogs": "new-only"
    },
    "format:fix": {
      "outputs": [".cache/.prettiercache"],
      "outputLogs": "new-only"
    },
    "lint": {
      "dependsOn": ["^topo", "^build"],
      "outputs": [".cache/.eslintcache"]
    },
    "lint:fix": {
      "dependsOn": ["^topo", "^build"],
      "outputs": [".cache/.eslintcache"]
    },
    "start": {
      "dependsOn": ["^start"],
      "cache": false,
      "persistent": false
    },
    "typecheck": {
      "dependsOn": ["^topo", "^build"],
      "outputs": [".cache/tsbuildinfo.json"]
    }
  },
  "globalEnv": [
    "ARCJET_KEY",
    "AUTH_APPLE_ID",
    "AUTH_APPLE_SECRET",
    "AUTH_BASE_URL",
    "AUTH_DISCORD_ID",
    "AUTH_DISCORD_SECRET",
    "AUTH_GOOGLE_ID",
    "AUTH_GOOGLE_SECRET",
    "AUTH_SECRET",
    "BASEHUB_TOKEN",
    "BETTERSTACK_UPTIME_API_KEY",
    "BLOB_READ_WRITE_TOKEN",
    "DATABASE_URL",
    "EDGE_CONFIG",
    "ENABLE_EXPERIMENTAL_COREPACK",
    "FLAGS_SECRET",
    "KV_REST_API_TOKEN",
    "KV_REST_API_URL",
    "KV_URL",
    "NEXT_PUBLIC_BLOB_URL",
    "NEXT_PUBLIC_POSTHOG_HOST",
    "NEXT_PUBLIC_POSTHOG_KEY",
    "NEXT_PUBLIC_SENTRY_DSN",
    "NEXT_PUBLIC_STATSIG_CLIENT_KEY",
    "PORT",
    "RESEND_FROM",
    "RESEND_KEY",
    "SENTRY_ORG",
    "SENTRY_PROJECT",
    "STATSIG_API_KEY",
    "STATSIG_CONSOLE_KEY",
    "STATSIG_EDGE_CONFIG_KEY",
    "STATSIG_EDGE_CONFIG",
    "STATSIG_PROJECT_ID"
  ],
  "globalPassThroughEnv": ["CI", "NODE_ENV", "npm_lifecycle_event"]
}
