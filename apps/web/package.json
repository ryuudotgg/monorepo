{
  "name": "@ryuu/web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./src/index.ts"
    }
  },
  "scripts": {
    "analyze": "ANALYZE=true pnpm build",
    "build": "pnpm with-env next build",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "pnpm with-env next dev --turbo",
    "format": "prettier --check . --ignore-path ../../.gitignore --ignore-path .gitignore",
    "postinstall": "fumadocs-mdx ./fumadocs.config.ts",
    "lint": "eslint",
    "start": "pnpm with-env next start",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "with-env": "dotenv -e ../../.env --"
  },
  "prettier": "@ryuu/prettier-config",
  "dependencies": {
    "@fumadocs/mdx-remote": "^1.2.0",
    "@hookform/resolvers": "^4.1.0",
    "@next-safe-action/adapter-react-hook-form": "^1.0.13",
    "@ryuu/analytics": "workspace:*",
    "@ryuu/auth": "workspace:*",
    "@ryuu/db": "workspace:*",
    "@ryuu/flags": "workspace:*",
    "@ryuu/nextjs": "workspace:*",
    "@ryuu/observability": "workspace:*",
    "@ryuu/security": "workspace:*",
    "@ryuu/shared": "workspace:*",
    "@ryuu/trpc": "workspace:*",
    "@ryuu/validators": "workspace:*",
    "@tanstack/react-query": "catalog:",
    "@trpc/client": "catalog:",
    "@trpc/react-query": "catalog:",
    "@vercel/speed-insights": "^1.2.0",
    "@vercel/toolbar": "^0.1.30",
    "fumadocs-core": "^15.0.9",
    "fumadocs-docgen": "^1.3.7",
    "fumadocs-mdx": "^11.5.6",
    "fumadocs-ui": "^15.0.8",
    "lucide-react": "^0.475.0",
    "next": "catalog:next",
    "next-safe-action": "^7.10.4",
    "oxc-transform": "^0.48.2",
    "react": "catalog:react19",
    "react-dom": "catalog:react19",
    "react-hook-form": "^7.54.2",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@ryuu/design": "workspace:*",
    "@ryuu/env": "workspace:*",
    "@ryuu/eslint-config": "workspace:*",
    "@ryuu/nextjs": "workspace:*",
    "@ryuu/prettier-config": "workspace:*",
    "@ryuu/tsconfig": "workspace:*",
    "@svgr/webpack": "^8.1.0",
    "@types/mdx": "^2.0.13",
    "@types/node": "catalog:",
    "@types/react": "catalog:react19",
    "@types/react-dom": "catalog:react19",
    "dotenv-cli": "^8.0.0",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  }
}
